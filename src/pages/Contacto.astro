---
import Layout from "../layouts/Layout.astro"
import Header from "../components/Header.astro"
import Footer from "../components/Footer.astro"
---

<Layout tittle="Coro Mozart Historico">
	<Header/>
	<main class="min-h-screen bg-black snap-mandatory">
		<section class="bg-slate-300 h-screen relative flex flex-col text-center text-white"  >
            <div class="bg-gray-800 xl:p-4 md:p-3 p-2 xl:fixed w-screen z-20 " style="top: 80px;">
                <h1 class="xl:text-6xl md:text-4xl text-2xl" style="font-family: BeVietnamPro;">Contacto</h1>
            </div>
            
        </section>
        
	</main>   
	<Footer/>
</Layout>